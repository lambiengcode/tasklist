<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hongvinhlcode.app.todolist">

    <uses-permission android:name="android.permission.ACCESS_NOTIFICATION_POLICY"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_v5"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_v5"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Main.Main.About.AboutActivity"
            android:parentActivityName=".Main.Main.MainActivity.MainActivity">
        </activity>
        <activity android:name=".Main.Main.Setting.SettingActivity" android:parentActivityName=".Main.Main.MainActivity.MainActivity"/>
        <activity
            android:name=".Main.Main.Create.CreateActivity"
            android:parentActivityName=".Main.Main.MainActivity.MainActivity" />
        <activity android:name=".Main.Main.MainActivity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>